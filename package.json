{
  "name": "ecr-psychology-assessment",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "dev": "vite",
    "dev:api": "vercel dev --listen 3000",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe-webhook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.5.0",
    "@vueuse/core": "^10.7.0",
    "chart.js": "^4.5.0",
    "echarts": "^5.6.0",
    "pinia": "^2.1.7",
    "stripe": "^18.3.0",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@stagewise-plugins/vue": "^0.6.2",
    "@stagewise/toolbar-vue": "^0.6.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@types/stripe": "^8.0.416",
    "@vercel/node": "^3.2.29",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vercel": "^37.14.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
